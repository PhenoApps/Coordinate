<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.wheatgenetics.coordinate">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:supportsRtl="true"
        android:theme="@style/OldAppTheme">

        <activity
            android:name=".main.OldMainActivity"
            android:configChanges="keyboardHidden|keyboard">

            <intent-filter>
                <action   android:name="android.intent.action.MAIN"       />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".tc.ExcludeCellsActivity">
            <intent-filter>
                <action   android:name="org.wheatgenetics.coordinate.tc.ExcludeCellsActivity" />
                <category android:name="android.intent.category.DEFAULT"                      />
            </intent-filter>
        </activity>

        <activity android:name=".preference.PreferenceActivity">
            <intent-filter>
                <action   android:name="org.wheatgenetics.coordinate.preference.PreferenceActivity" />
                <category android:name="android.intent.category.DEFAULT"                            />
            </intent-filter>
        </activity>
    </application>
</manifest>